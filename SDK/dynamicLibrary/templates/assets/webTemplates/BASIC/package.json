{
  "name": "furhat-skill-gui",
  "version": "1.0.0",
  "description": "GUI Template for furhat skill",
  "main": "src/index.js",
  "scripts": {
    "start": "npm-run-all --parallel dev:server lint:watch",
    "dev:server": "webpack-dev-server --hot --inline",
    "watch": "webpack -w -d",
    "build": "webpack",
    "lint": "node_modules/.bin/esw webpack.config.* src --color",
    "lint:watch": "npm run lint -- --watch"
  },
  "keywords": [
    "es6"
  ],
  "author": "Furhat Robotics",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^3.5.0",
    "eslint-watch": "^2.1.14",
    "npm-run-all": "^4.0.2",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "furhat-core": "0.0.3",
    "furhat-gui": "0.0.5"
  }
}
